{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "singularName": "blog",
    "pluralName": "blogs",
    "displayName": "Blogs",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string",
      "required": true,
      "unique": false,
      "maxLength": 255
    },
    "Category": {
      "type": "enumeration",
      "enum": [
        "Business",
        "Travel",
        "Sport",
        "Education",
        "Politics",
        "Entertainment",
        "Finance"
      ],
      "required": true
    },
    "Summary": {
      "type": "text",
      "maxLength": 88,
      "required": true
    },
    "IsFeatured": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "Thumbnail": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "TimeStamp": {
      "type": "datetime",
      "required": true
    },
    "author": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::author.author",
      "inversedBy": "blogs"
    },
    "Content": {
      "type": "blocks",
      "required": true
    },
    "slug": {
      "type": "uid",
      "required": false,
      "targetField": "Title"
    },
    "FeaturedImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "videos"
      ]
    }
  }
}
